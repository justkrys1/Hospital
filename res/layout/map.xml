    <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent" android:background="#a5acb5">
        <com.qozix.tileview.TileView
                android:layout_width="match_parent"
                android:layout_height="1013dp"
                android:id="@+id/tileView1"
                android:layout_alignParentStart="true" android:background="#a5acb5"/>
    </RelativeLayout>
</LinearLayout>



